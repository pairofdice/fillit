int test_parse_piece()
{
	int i;
	int k;
	char test_input[22];
	t_piece piece;
	char	*tests[] = {
		"one_piece1",
		"one_piece2",
		"one_piece3",
		"one_piece4",
		"one_piece5",
		0};

	i = 0;
	while (tests[i])
	{
		if (i > 0)
			printf("-------\n");
		read(open(tests[i], O_RDONLY), test_input, 22);
		test_input[21] = '\0';
		printf("%s\n",test_input);
		parse_piece(test_input, &piece);
		k = 0;
		while (k < 3)
		{
			printf("% ld, % ld\n", piece.offsets[k*2], piece.offsets[k*2 + 1]);
			k++;
		}
		i++;
	}
	return (0);
}

int test_parse()
{
	int i;
	int k;
	char test_input[100];
	t_piece pieces[4];
	char	*tests[] = {
		"three_L",
		0};

	i = 0;
	while (tests[i])
	{
		if (i > 0)
			printf("-------\n");
		read(open(tests[i], O_RDONLY), test_input, 99);
		test_input[99] = '\0';
		printf("%s\n",test_input);
		ft_parse(test_input, &pieces[i], 3);
		k = 0;
		while (k < 3)
		{
			printf("% ld, % ld\n", pieces[i].offsets[k*2], pieces[i].offsets[k*2 + 1]);
			k++;
		}
		i++;
	}
	return (0);
}

